{
    "expo": {
    "name": "NoWoo",
    "slug": "nowoo",
    "scheme": "com.nowoo.app",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/ios-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2d3748"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/ios-icon.png",
      "buildNumber": "17",
      "bundleIdentifier": "com.nowoo.app"
    },
    "android": {
      "blockedPermissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "com.google.android.finsky.permission.BIND_GET_INSTALL_REFERRER_SERVICE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-adaptive-icon-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/android-icon.png",
      "package": "com.nowoo.app",
      "versionCode": 3145747
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8a2dbb83-3bcc-4d88-9d99-5fd3220273ea"
      }
    },
    "androidStatusBar": {
      "hidden": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ],
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "overlay-swipe",
          "backgroundColor": "#00000080"
        }
      ],
      "expo-apple-authentication"
    ],
    "notification": {
      "icon": "./assets/ios-icon.png",
      "color": "#2d3748",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "NoWoo"
    }
  }
}
